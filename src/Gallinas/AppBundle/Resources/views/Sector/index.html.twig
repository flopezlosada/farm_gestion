    {% extends '::layout_list.html.twig' %}

    {% block title_wrapper %}Listado {% endblock %}
    {% block main -%}
    <h1>Sector list</h1>

    <table class="records_list">
        <thead>
        <tr>
                <th>Id</th>
                <th>Name</th>
            <th>Actions</th>
        </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
        <tr>
                    <td>
                        <a href="{{ path('sector_show', { 'id': entity.id }) }}">{{ entity.id }}</a>
                    </td>
                    <td>{{ entity.name }}</td>
                    <td>
                <ul>
                    <li>
                        <a href="{{ path('sector_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('sector_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                    </td>
        </tr>
        {% endfor %}
        </tbody>
    </table>

            <ul>
            <li>
                <a href="{{ path('sector_new') }}">
                    Create a new entry
                </a>
            </li>
        </ul>
        {% endblock %}

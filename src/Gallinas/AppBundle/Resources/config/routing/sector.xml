<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="sector" path="/">
        <default key="_controller">AppBundle:Sector:index</default>
    </route>

    <route id="sector_show" path="/{id}/show">
        <default key="_controller">AppBundle:Sector:show</default>
    </route>

    <route id="sector_new" path="/new">
        <default key="_controller">AppBundle:Sector:new</default>
    </route>

    <route id="sector_create" path="/create" methods="POST">
        <default key="_controller">AppBundle:Sector:create</default>
    </route>

    <route id="sector_edit" path="/{id}/edit">
        <default key="_controller">AppBundle:Sector:edit</default>
    </route>

    <route id="sector_update" path="/{id}/update" methods="POST|PUT">
        <default key="_controller">AppBundle:Sector:update</default>
    </route>

    <route id="sector_delete" path="/{id}/delete" methods="POST|DELETE">
        <default key="_controller">AppBundle:Sector:delete</default>
    </route>

</routes>

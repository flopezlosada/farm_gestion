<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="culturalworktype" path="/">
        <default key="_controller">AppBundle:CulturalWorkType:index</default>
    </route>

    <route id="culturalworktype_show" path="/{id}/show">
        <default key="_controller">AppBundle:CulturalWorkType:show</default>
    </route>

    <route id="culturalworktype_new" path="/new">
        <default key="_controller">AppBundle:CulturalWorkType:new</default>
    </route>

    <route id="culturalworktype_create" path="/create" methods="POST">
        <default key="_controller">AppBundle:CulturalWorkType:create</default>
    </route>

    <route id="culturalworktype_edit" path="/{id}/edit">
        <default key="_controller">AppBundle:CulturalWorkType:edit</default>
    </route>

    <route id="culturalworktype_update" path="/{id}/update" methods="POST|PUT">
        <default key="_controller">AppBundle:CulturalWorkType:update</default>
    </route>

    <route id="culturalworktype_delete" path="/{id}/delete" methods="POST|DELETE">
        <default key="_controller">AppBundle:CulturalWorkType:delete</default>
    </route>

</routes>

<?xml version="1.0" encoding="UTF-8" ?>

<routes xmlns="http://symfony.com/schema/routing"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/routing http://symfony.com/schema/routing/routing-1.0.xsd">

    <route id="fowlstatus" path="/">
        <default key="_controller">AppBundle:FowlStatus:index</default>
    </route>

    <route id="fowlstatus_show" path="/{id}/show">
        <default key="_controller">AppBundle:FowlStatus:show</default>
    </route>

    <route id="fowlstatus_new" path="/new">
        <default key="_controller">AppBundle:FowlStatus:new</default>
    </route>

    <route id="fowlstatus_create" path="/create">
        <default key="_controller">AppBundle:FowlStatus:create</default>
        <requirement key="_method">post</requirement>
    </route>

    <route id="fowlstatus_edit" path="/{id}/edit">
        <default key="_controller">AppBundle:FowlStatus:edit</default>
    </route>

    <route id="fowlstatus_update" path="/{id}/update">
        <default key="_controller">AppBundle:FowlStatus:update</default>
        <requirement key="_method">post|put</requirement>
    </route>

    <route id="fowlstatus_delete" path="/{id}/delete">
        <default key="_controller">AppBundle:FowlStatus:delete</default>
        <requirement key="_method">post|delete</requirement>
    </route>

</routes>
